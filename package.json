{
  "name": "travi",
  "description": "Matt Travi",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "travi.json",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/npm-travi",
  "bugs": "https://github.com/travi/npm-travi/issues",
  "homepage": "https://github.com/travi/npm-travi#readme",
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:json": "jsonlint travi.json",
    "lint:schema": "hackmyresume VALIDATE --assert travi.json",
    "generate:md": "remark . --output",
    "test": "run-s lint:*",
    "preversion": "npm test",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "globstar": "1.0.0",
    "hackmyresume": "1.8.0",
    "husky": "9.0.11",
    "jsonlint": "1.6.3",
    "npm-run-all2": "6.1.2",
    "remark-cli": "12.0.0",
    "remark-preset-lint-travi": "6.0.4",
    "validate-commit-msg": "2.14.0"
  }
}
